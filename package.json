{
  "name": "babel-timing",
  "version": "0.8.0",
  "description": "Measure Babel compilation time",
  "main": "src/index.js",
  "files": [
    "src",
    "webpack",
    "jest",
    "cli.js",
    "!__tests__",
    "!__mocks__",
    "!__fixtures__",
    "!__utils__"
  ],
  "bin": "./cli.js",
  "scripts": {
    "test": "jest",
    "start": "node cli.js __fixtures__/entry.js --follow-imports",
    "preversion": "npm run prepublish",
    "version": "git add package.json",
    "postversion": "git push && git push --tags",
    "prepublish": "npm t",
    "format": "prettier --write \"**/*.{js,json,md}\""
  },
  "keywords": [
    "babel",
    "profiler",
    "performance",
    "measurement",
    "time"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toomuchdesign/babel-timing.git"
  },
  "author": "Andrea Carraro <me@andreacarraro.it>",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.9.0",
    "ansi-diff-stream": "^1.2.1",
    "babel-loader": "^8.1.0",
    "cli-table3": "^0.6.0",
    "colors": "^1.4.0",
    "commander": "^4.1.1",
    "find-babel-config": "^1.2.0",
    "find-cache-dir": "^3.3.1",
    "glob": "^7.1.6",
    "lodash.chunk": "^4.2.0",
    "lodash.defaults": "^4.2.0",
    "lodash.mergewith": "^4.6.2",
    "minimatch": "^3.0.4",
    "multimatch": "^4.0.0",
    "reduce-flatten": "^3.0.0",
    "rimraf": "^3.0.2",
    "webpack": "^4.43.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "babel-jest": "^24.9.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "webpack-cli": "^3.3.12"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "__fixtures__",
      "__utils__"
    ],
    "watchPathIgnorePatterns": [
      ".*\\.temp.*"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,json}": [
      "prettier --write",
      "npm t -- .",
      "git add"
    ],
    "**/*.{md}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=10.0"
  }
}
