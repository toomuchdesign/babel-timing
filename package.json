{
  "name": "babel-timing",
  "version": "0.9.1",
  "description": "Measure Babel compilation time",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "webpack",
    "jest"
  ],
  "bin": "./dist/cli.js",
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "npm run clean && tsc",
    "test": "jest test.{j,t}s",
    "test:compiled": "jest test.compiled.{j,t}s",
    "start": "node ./dist/cli.js __fixtures__/entry.js --follow-imports",
    "preversion": "npm run prepublish",
    "version": "git add package.json",
    "postversion": "git push && git push --tags",
    "prepublish": "npm run compile && jest",
    "format": "prettier --write \"**/*.{js,ts,json,md}\""
  },
  "keywords": [
    "babel",
    "profiler",
    "performance",
    "measurement",
    "time"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toomuchdesign/babel-timing.git"
  },
  "author": "Andrea Carraro <me@andreacarraro.it>",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.14.6",
    "ansi-diff-stream": "^1.2.1",
    "babel-loader": "^8.2.2",
    "cli-table3": "^0.6.0",
    "colors": "^1.4.0",
    "commander": "^7.2.0",
    "find-babel-config": "^1.2.0",
    "find-cache-dir": "^3.3.1",
    "glob": "^7.1.7",
    "lodash.chunk": "^4.2.0",
    "lodash.defaults": "^4.2.0",
    "lodash.mergewith": "^4.6.2",
    "minimatch": "^3.0.4",
    "multimatch": "^5.0.0",
    "rimraf": "^3.0.2",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@types/find-cache-dir": "^3.2.0",
    "@types/jest": "^26.0.23",
    "@types/lodash.chunk": "^4.2.6",
    "@types/lodash.defaults": "^4.2.6",
    "@types/lodash.mergewith": "^4.6.6",
    "@types/rimraf": "^3.0.0",
    "@types/webpack": "^4.41.29",
    "babel-jest": "^26.6.3",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.3",
    "webpack-cli": "^3.3.12"
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-ts",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "__fixtures__",
      "__utils__"
    ],
    "watchPathIgnorePatterns": [
      ".*\\.temp.*"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run compile && lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,json}": [
      "prettier --write",
      "npm t -- ."
    ],
    "**/*.{md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=11.0"
  }
}
