{
  "name": "babel-timing",
  "version": "0.5.0",
  "description": "Measure Babel compilation time",
  "main": "src/index.js",
  "files": [
    "src",
    "webpack",
    "jest",
    "cli.js",
    "!__tests__",
    "!__mocks__",
    "!__fixtures__",
    "!__utils__"
  ],
  "bin": "./cli.js",
  "scripts": {
    "test": "jest",
    "preversion": "npm run prepublish",
    "version": "git add package.json",
    "postversion": "git push && git push --tags",
    "prepublish": "npm t",
    "format": "prettier --write \"**/*.{js,json,md}\""
  },
  "keywords": [
    "babel",
    "performance",
    "compilation",
    "performance",
    "measurement",
    "time"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toomuchdesign/babel-timing.git"
  },
  "author": "Andrea Carraro <me@andreacarraro.it>",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.4.3",
    "ansi-diff-stream": "^1.2.1",
    "cli-table3": "^0.5.1",
    "colors": "^1.3.3",
    "commander": "^2.20.0",
    "find-babel-config": "^1.2.0",
    "glob": "^7.1.3",
    "lodash.chunk": "^4.2.0",
    "lodash.mergewith": "^4.6.1",
    "multimatch": "^4.0.0",
    "reduce-flatten": "^2.0.0",
    "rollup": "^1.10.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "husky": "^1.0.0",
    "jest": "^24.7.1",
    "lint-staged": "^7.0.0",
    "prettier": "^1.0.0",
    "rimraf": "^2.6.3",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "__fixtures__",
      "__utils__"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,json}": [
      "prettier --write",
      "npm t -- .",
      "git add"
    ],
    "**/*.{md}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=7.6"
  }
}
